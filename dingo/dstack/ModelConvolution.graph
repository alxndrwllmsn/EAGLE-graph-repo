            "fields": [
                {
                    "name": "func_name",
                    "value": "convolve_model",
                    "defaultValue": "",
                    "description": "Python function name",
                    "readonly": false,
                    "type": "String",
                    "precious": false,
                    "options": [],
                    "positional": false,
                    "encoding": "pickle",
                    "id": "88f2f8c0-faf4-4bd0-bb73-ab3763b69359",
                    "parameterType": "ComponentParameter",
                    "usage": "NoPort"
                },
                {
                    "name": "func_code",
                    "value": "def convolve_model(model, gaussian, weight):\n    import numpy as np\n    conv_model = np.zeros(model.shape)\n    gaussian_f = np.fft.fft2(gaussian)\n    for i in range(conv_model.shape[0]):\n        model_f = np.fft.fft2(model[i,...])\n        conv_model_f = model_f*gaussian_f\n        conv_model[i,...] = np.fft.ifftshift(np.fft.ifft2(conv_model_f))\n    return conv_model",
                    "defaultValue": "",
                    "description": "Python function code, e.g. 'def func_name(args): return args'. Note that func_name above needs to match the defined name here.",
                    "readonly": false,
                    "type": "String",
                    "precious": false,
                    "options": [],
                    "positional": false,
