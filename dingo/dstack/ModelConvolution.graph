            "fields": [
                {
                    "name": "func_name",
                    "value": "read_psf",
                    "defaultValue": "",
                    "description": "Python function name",
                    "readonly": false,
                    "type": "String",
                    "precious": false,
                    "options": [],
                    "positional": false,
                    "encoding": "pickle",
                    "id": "0b263092-1acf-4ab5-b293-a7d6cb16902d",
                    "parameterType": "ComponentParameter",
                    "usage": "NoPort"
                },
                {
                    "name": "func_code",
                    "value": "def read_psf(base_filename : str):\n    import adios2\n    import numpy as np\n    base_filename = base_filename.split(',')\n    fname_r = f\"{base_filename[0]}/psfgrid.{base_filename[1]}.real/table.f0.bp\"\n    fname_i = f\"{base_filename[0]}/psfgrid.{base_filename[1]}.imag/table.f0.bp\"\n    with adios2.FileReader(fname_r) as f:\n        mapvars = f.available_variables()[\"map\"]\n        shape = [int(x) for x in mapvars[\"Shape\"].split(',')[1:]]\n        data = f.read(\"map\", start=(0,shape[0]//2,0,0,0), count=(1,1,1,shape[2],shape[3])).reshape(shape[2],shape[3]).astype(np.complex64)\n    with adios2.FileReader(fname_i) as f:\n        mapvars = f.available_variables()[\"map\"]\n        shape = [int(x) for x in mapvars[\"Shape\"].split(',')[1:]]\n        data.imag = f.read(\"map\", start=(0,shape[0]//2,0,0,0), count=(1,1,1,shape[2],shape[3])).reshape(shape[2],shape[3])\n\n    psf = np.fft.fftshift(np.fft.fft2(data))\n    psf /= psf.max()\n    return psf",
                    "defaultValue": "",
